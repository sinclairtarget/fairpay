<%= form_tag controller: "salaries", action: "create" do %>
  <%= label_tag :title_label, "Title" %>
  <%= text_field_tag :title %>
  <%= label_tag :salary_label, "Annual Salary" %>
  <%= number_field :salary, :annual_pay %>
  <%= hidden_field_tag :group_id, @group_id %>
  <%= submit_tag "Submit" %> 
<% end %>
