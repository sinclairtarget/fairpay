<div class="group-listing" data-id="<%= group.id %>">
  <div class="group-listing-header">
    <div class="inline">
      <h3><%= group.name %></h3>
    </div>
    <div class="inline member-count">
      <span class="glyphicon glyphicon-user"></span> <%= group.users.count %>
    </div>
  </div>
  <div class="group-listing-body">
    <table>
      <tr>
        <td><h5>Your Title:</h5></td>
        <td><%= salary.title %></td>
      </tr>
      <tr>
        <td><h5>Your Salary:</h5></td>
        <td><%= dollar(salary.annual_pay) %></td>
      </tr>
    </table>
  </div>
</div>
